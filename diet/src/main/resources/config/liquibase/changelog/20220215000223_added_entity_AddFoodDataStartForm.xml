    <!--
        Added the entity AddFoodDataStartForm.
    -->
    <changeSet id="20220215000223-1" author="jhipster">
        <createTable tableName="add_food_data_start_form">
            <column name="id" type="bigint">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="name" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="grams" type="${floatType}">
                <constraints nullable="true" />
            </column>
            <column name="protein" type="${floatType}">
                <constraints nullable="true" />
            </column>
            <column name="carbohydrate" type="${floatType}">
                <constraints nullable="true" />
            </column>
            <column name="fat" type="${floatType}">
                <constraints nullable="true" />
            </column>
            <column name="calories" type="${floatType}">
                <constraints nullable="true" />
            </column>
            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here -->
        </createTable>
    </changeSet>

    <!-- jhipster-needle-liquibase-add-changeset - JHipster will add changesets here -->
